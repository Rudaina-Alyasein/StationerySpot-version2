@model stationerySpot.Models.LibraryRegistrationRequest

@{
    Layout = "HomeLayout";
}
@section Style{
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600;700&display=swap" rel="stylesheet">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <!-- Bootstrap CSS -->
    @* <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"> *@
    <!-- Add Swiper CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.css">
    <style>
        :root {
            --primary-color: #0097b2;
            --secondary-color: #fa65b1;
            --light-bg: #f8f9fa;
            --gradient: linear-gradient(135deg, #0097b2 0%, #fa65b1 100%);
        }

        body {
            font-family: 'Poppins', sans-serif;
            color: #333;
        }

        /* Hero Section */
        .hero-section {
            position: relative;
            height: 600px;
            overflow: hidden;
        }

        .hero-video-container {
            width: 100%;
            height: 100%;
        }

        .hero-img-container {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .hero-content-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.5);
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .hero-content {
            text-align: center;
            color: white;
            max-width: 800px;
            padding: 2rem;
        }

            .hero-content h2 {
                font-size: 2.5rem;
                margin-bottom: 1.5rem;
            }

            .hero-content p {
                font-size: 1.2rem;
                margin-bottom: 2rem;
            }

        .cta-buttons {
            display: flex;
            gap: 1rem;
            justify-content: center;
        }

        .cta-btn {
            padding: 1rem 2rem;
            border-radius: 30px;
            text-decoration: none;
            font-weight: 600;
            transition: all 0.3s ease;
        }

            .cta-btn:first-child {
                background: var(--primary-color);
                color: white;
            }

            .cta-btn.secondary {
                background: transparent;
                border: 2px solid white;
                color: white;
            }

        /* About Section */
        .about-section {
            padding: 4rem 0;
        }

            .about-section img {
                max-width: 100%;
                height: auto;
            }

        /* How It Works Section */
        .how-it-works {
            background: var(--light-bg);
            padding: 4rem 0;
        }

        .step-icon {
            color: var(--primary-color);
            margin-bottom: 1rem;
        }

        /* Testimonials Section */
        .testimonial-section {
            padding: 4rem 0;
            background: var(--light-bg);
        }

        .testimonial-card {
            background: white;
            border-radius: 15px;
            padding: 2rem;
            margin: 1rem;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
            height: 400px;
            display: flex;
            flex-direction: column;
        }

            .testimonial-card .card-body {
                height: 100%;
                display: flex;
                flex-direction: column;
            }

            .testimonial-card .row {
                height: 100%;
            }

            .testimonial-card .col-lg-4 {
                display: flex;
                align-items: center;
                justify-content: center;
            }

            .testimonial-card img {
                width: 200px;
                height: 200px;
                object-fit: cover;
                border-radius: 50%;
            }

            .testimonial-card .col-lg-8 {
                display: flex;
                flex-direction: column;
                justify-content: center;
            }

        /* Join Us Section */
        .join-us {
            padding: 4rem 0;
        }

            .join-us .card {
                height: 300px;
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: center;
                text-align: center;
                padding: 2rem;
            }

            .join-us .card-body {
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: center;
            }

            .join-us .card i {
                color: var(--primary-color);
                margin-bottom: 1.5rem;
            }

        /* Registration Form */
        .registration-form {
            padding: 4rem 0;
            margin-bottom: 5rem;
        }

            .registration-form .form-outline {
                position: relative;
                margin-bottom: 1.5rem;
            }

            .registration-form .form-label {
                position: absolute;
                top: -0.5rem;
                left: 0.75rem;
                background: white;
                padding: 0 0.5rem;
                font-size: 0.875rem;
                color: var(--primary-color);
                font-weight: 500;
            }

            .registration-form .form-control {
                border: 2px solid #eee;
                padding: 1rem;
                border-radius: 8px;
                height: 3.5rem;
            }

                .registration-form .form-control:focus {
                    border-color: var(--primary-color);
                    box-shadow: none;
                }

            .registration-form textarea.form-control {
                height: auto;
                min-height: 100px;
            }

            .registration-form select.form-control {
                padding-top: 1.5rem;
            }

            .registration-form .form-control:focus + .form-label,
            .registration-form .form-control:not(:placeholder-shown) + .form-label {
                color: var(--primary-color);
            }

            .registration-form .form-control::placeholder {
                color: transparent;
            }

            .registration-form img {
                max-width: 100%;
                height: auto;
                object-fit: contain;
            }

        /* FAQ Section */
        .faq-container {
            padding: 4rem 0;
            margin-bottom: 5rem;
        }

            .faq-container img {
                max-width: 100%;
                height: auto;
                object-fit: contain;
            }

        /* Libraries Section */
        .libraries {
            padding: 4rem 0;
        }

        .stat-card {
            background: white;
            border-radius: 15px;
            padding: 2rem;
            margin: 1rem;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
            height: 400px;
            display: flex;
            flex-direction: column;
            align-items: center;
        }

            .stat-card img {
                width: 200px;
                height: 200px;
                object-fit: cover;
                border-radius: 50%;
                margin-bottom: 1rem;
            }

            .stat-card h5 {
                color: var(--primary-color);
                margin-bottom: 1rem;
            }

            .stat-card p {
                color: #666;
                margin-bottom: 1rem;
                flex-grow: 1;
                text-align: center;
            }

        /* Section Margins */
        section {
            margin-bottom: 5rem;
        }

        
    </style>
}
<section class="hero-section">
    <div class="hero-video-container">
        <img src="~/assets\img\Frame 1 (1).png" class="hero-img-container">
    </div>

    <div class="hero-content-overlay">
        <div class="hero-content">
            <h2>Join the Largest Platform for Local Libraries and Expand Your Reach</h2>
            <p>
                It's time to make your library part of a unique shopping experience. Gain access to new customers,
                boost your sales, and become part of a growing network of libraries.
            </p>

            <div class="cta-buttons">
                <a href="#join-us1" class="cta-btn">Join Now</a>
                <a href="#about" class="cta-btn secondary">Learn More</a>
            </div>
        </div>
    </div>
</section>

<section class="about-section py-3 py-md-5" id="about">
    <div class="container">
        <div class="row gy-3 gy-md-4 gy-lg-0 align-items-lg-center">
            <div class="col-12 col-lg-6 col-xl-5">
                <img class="img-fluid rounded" loading="lazy"
                     src="~/assets\img\Premium_Vector___Art_Concept_With_Kids_On_Tree-removebg-preview.png"
                     alt="About 1">
            </div>
            <div class="col-12 col-lg-6 col-xl-7">
                <div class="row justify-content-xl-center">
                    <div class="col-12 col-xl-11">
                        <h2 class="mb-3">Who Are We?</h2>
                        <p class="lead fs-4 text-secondary mb-3">
                            We are a platform dedicated to connecting local libraries with a broader community. Our
                            mission is to help libraries expand their reach, increase their visibility, and enhance
                            their offerings by connecting them with new customers online.
                        </p>
                        <p class="mb-5 text-secondary">
                            We offer a unique platform where local libraries can showcase their collections, offer
                            products, and connect with a growing network of customers. We believe in collaboration,
                            innovation, and customer satisfaction, and we are constantly working to improve the
                            services we provide to libraries and their patrons.
                        </p>

                        <h2 class="mb-3">How We Earn ?</h2>
                        <p class="text-secondary">
                            Our platform operates on a commission-based model, ensuring that libraries can list
                            their products with no upfront costs. We take a small commission of 15% on each
                            successful transaction, allowing us to maintain and improve the platform while providing
                            the best service to both libraries and customers.
                        </p>
                        <p class="text-secondary">
                            Libraries receive their earnings directly to their preferred payment method, with
                            transparent reporting and no hidden fees.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

