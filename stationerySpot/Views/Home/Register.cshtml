<section id="join-us1">
    <div class="container h-100">
        <div class="row d-flex justify-content-center align-items-center h-100">
            <div class="col-lg-12 col-xl-11">
                <div class="card text-black" style="border-radius: 25px;">
                    <div class="card-body p-md-5" style="box-shadow: none !important;">
                        <div class="row justify-content-center">
                            <div class="col-md-10 col-lg-6 col-xl-5 order-2 order-lg-1">
                                <p class="text-center h1 fw-bold mb-5 mx-1 mx-md-4 mt-4">Register your library</p>

                                <form method="POST" enctype="multipart/form-data" class="mx-1 mx-md-4">
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <input type="text" id="LibraryName" name="LibraryName"
                                                   class="form-control" />
                                            <label class="form-label" for="LibraryName">Library name</label>
                                            @Html.ValidationMessageFor(model => model.LibraryName, "", new
                                                {
                                                    @class
                                                     = "text-danger"
                                                })
                                        </div>
                                    </div>

                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <input type="text" id="Location" name="Location" class="form-control" />
                                            <label class="form-label" for="Location">Library address</label>
                                            @Html.ValidationMessageFor(model => model.Location, "", new
                                                {
                                                    @class =
                                                     "text-danger"
                                                })
                                        </div>
                                    </div>

                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <input type="email" id="Email" name="Email" class="form-control" />
                                            <label class="form-label" for="Email">Email</label>
                                            @Html.ValidationMessageFor(model => model.Email, "", new
                                                {
                                                    @class =
                                                     "text-danger"
                                                })
                                        </div>
                                    </div>

                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <input type="text" id="Phone" name="Phone" class="form-control" />
                                            <label class="form-label" for="Phone">Phone</label>
                                            @Html.ValidationMessageFor(model => model.Phone, "", new
                                                {
                                                    @class =
                                                     "text-danger"
                                                })
                                        </div>
                                    </div>

                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <input type="text" id="Specialization" name="Services"
                                                   class="form-control" />
                                            <label class="form-label" for="Specialization">
                                                What does your library
                                                specialize in?
                                            </label>
                                        </div>
                                    </div>

                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <textarea id="Description" name="Description" class="form-control"
                                                      rows="4"></textarea>
                                            <label class="form-label" for="Description">
                                                Describe your
                                                library
                                            </label>
                                        </div>
                                    </div>

                                    <!-- Dropdown for City -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <select id="City" name="City" class="form-control">
                                                <option value="" disabled selected>Select City</option>
                                                <option value="Amman">Amman</option>
                                                <option value="Irbid">Irbid</option>
                                                <option value="Zarqa">Zarqa</option>
                                                <option value="Aqaba">Aqaba</option>
                                                <option value="Madaba">Madaba</option>
                                                <option value="Mafraq">Mafraq</option>
                                                <option value="Karak">Karak</option>
                                                <option value="Tafilah">Tafilah</option>
                                                <option value="Jerash">Jerash</option>
                                                <option value="Ajloun">Ajloun</option>
                                                <option value="Salt">Salt</option>
                                                <option value="Balqa">Balqa</option>
                                            </select>
                                            <label class="form-label" for="City">City</label>
                                        </div>
                                    </div>

                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <div data-mdb-input-init class="form-outline flex-fill mb-0">
                                            <input type="file" id="LogoPathFile" name="LogoPathFile"
                                                   class="form-control" />
                                            <label class="form-label" for="LogoPath">Library Logo</label>
                                        </div>
                                    </div>

                                    <div class="d-flex justify-content-center mx-4 mb-3 mb-lg-4">
                                        <button type="submit" data-mdb-button-init data-mdb-ripple-init
                                                class="btn btn-primary btn-lg"
                                                style="background-color: #f0f0f0 !important; border:1px solid #fa65b1!important; color:#fa65b1">
                                            Register
                                        </button>
                                    </div>
                                </form>
                                <!-- عرض الأخطاء العامة هنا -->
                                <div>
                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                </div>

                            </div>
                            @if (TempData["SuccessMessage"] != null)
                            {
                                <div class="alert alert-success alert-dismissible fade show" role="alert">
                                    @TempData["SuccessMessage"]
                                    <button type="button" class="btn-close" data-bs-dismiss="alert"
                                            aria-label="Close"></button>
                                </div>
                            }

                            <div class="col-md-10 col-lg-6 col-xl-7 d-flex align-items-center order-1 order-lg-2">
                                <img src="~/assets/img/download__5_-removebg-preview.png" style="margin-left: 60px;"
                                     class="img-fluid" alt="Sample image">
                                @Html.ValidationMessageFor(model => model.LogoPath, "", new
                                    {
                                        @class = "text-danger"
                                    })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="row faq-container pt-md-0">
    <div class="col-12 col-md-4">
        <h6 style="text-align:center; font-weight:bold; font:38px;">Popular Questions</h6>
        <img src="~/assets\img\download__7_-removebg-preview.png">
    </div>
    <div class="col-12 col-md-8">
        <div class="accordion" id="accordionExample">
